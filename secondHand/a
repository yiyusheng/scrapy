 <script>
 var cid = 0;
 var page = 1;
  $(document).ready(function(){
	  getList(0,1);
  });

  function getList(cid,page){
	   var url = "sale/getList/"+cid+"?page="+page;
		$.ajax({
			url:url,
			async:true,
			beforeSend:function(){
				common._loadding('.idle-content');
			},
			complete:function(){
				common._loaddingClose();
			},
			success:function(data){
				if(data.status=='success'){
    				var html = "";
    				$.each(data.data.dataList,function(i,el){
    					html+= template('list-tmpl',el);
    				});
    				if(data.data.is_last_page==1){
    					$("#idleMore").hide();
    				}else{
    					$("#idleMore").show();
        			}
        			
    				$(".idle-content-list").append(html);
				}
		}});  
  }

  $(function(){
	    //tab 切换
	    idleIndexFun._idle_content_nav();
	     //加载更多
	    idleIndexFun._idle_more();
	   
	})

	var idleIndexFun={
	    _idle_content_nav(){ //tab 切换
	        $('.idle-content-nav li').click(function(){
	            $(this).addClass('active').siblings().removeClass('active')
	            cid = $(this).attr("data_id");
	            page = 1;
	            $(".idle-content-list").empty();
	            getList(cid,page);
	        })
	    },
	     _idle_more(){ //加载更多
	        var flag=true;
	        $('#idleMore').click(function(){
	         	page = page+1;
	         	getList(cid,page);
	        })
	     }
	}
</script>
<script type="text/javascript">
	$("#add_sale").click(function(){
		var user_id = 0;
		if(!user_id){
			headerFun._showLogin();
		}else{
			window.location.href = '/sale/add';
		}		
	});
</script>
